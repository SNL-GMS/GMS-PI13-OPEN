    "apollo-link-dedup": "1.0.20",
    "apollo-link-error": "1.1.12",
    "apollo-link-http": "1.5.16",
    "apollo-link-logger": "1.2.3",
    "apollo-link-ws": "1.0.19",
    "apollo-utilities": "1.3.2",
    "graphql": "14.5.8",
    "lodash": "4.17.15",
    "log4javascript": "1.4.15",
    "msgpack-lite": "0.1.26",
    "react": "16.12.0",
    "subscriptions-transport-ws": "0.9.16",
    "transit-js": "0.8.861"
  },
