{
  "name": "@gms/ui-apollo",
  "version": "0.1.0",
  "description": "GMS UI Apollo Configuration",
  "author": "GMS Development Team",
  "license": "UNLICENSED",
  "files": [
    "lib",
    "dist"
  ],
  "main": "./dist/ui-apollo.js",
  "module": "./lib/ui-apollo.js",
  "typings": "./lib/ui-apollo.d.ts",
  "scripts": {
    "clean:node_modules": "rimraf node_modules/",
    "clean:build-scripts": "rimraf webpack-config/",
    "clean:dist": "rimraf build/ && rimraf dist/ && rimraf lib/ && rimraf bundle-analyzer/",
    "clean:coverage": "rimraf coverage/",
    "clean:docs": "rimraf docs/",
    "clean": "npm-run-all -p clean:build-scripts clean:dist clean:coverage clean:docs -p clean:node_modules",
    "build:build-scripts": "yarn clean:build-scripts && cp -R ../../../webpack-config/ ./webpack-config/",
    "build:tslint": "yarn tslint --project ./tsconfig.json --config ./tslint.json 'src/**/*.{ts,tsx}' --fix",
    "build:tslint:test": "yarn tslint --project ./tsconfig-test.json --config ./tslint.json '{__tests__,__mocks__}/**/*.{ts,tsx}' --fix",
    "build:tsc-defs": "tsc --project ./tsconfig.json --emitDeclarationOnly",
    "build:tsc:dev": "tsc --project ./tsconfig.json --sourceMap true --declaration true --declarationMap true",
    "build:tsc:prod": "tsc --project ./tsconfig.json --sourceMap false --declaration true --declarationMap false",
    "build:tsc:test": "tsc --project ./tsconfig-test.json",
    "build:webpack:dev": "webpack --colors --env.development --config webpack.config.ts",
    "build:webpack:prod": "webpack --colors --env.production --config webpack.config.ts",
    "build:dev": "npm-run-all -p clean:dist build:build-scripts -p build:tsc:dev",
    "build:prod": "npm-run-all -p clean:dist build:build-scripts -p build:tsc:prod",
    "build": "yarn build:dev",
    "bundle:dev": "yarn build:webpack:dev",
    "bundle:prod": "yarn build:webpack:prod",
    "bundle": "yarn bundle:dev",
    "watch": "yarn build:tsc:dev --watch --preserveWatchOutput",
    "docs": "yarn clean:docs && typedoc --out docs ./src/ --name ui-apollo --mode file --readme README.md --target ES6 --includeDeclarations --excludeExternals --excludeNotExported --hideGenerator",
    "sonar": "echo \"run 'yarn sonar' from the top-level directory\"",
    "test": "yarn test:dev",
    "test:dev": "yarn build:tsc:test && jest --config jest.config.js --silent false --verbose=true --bail=true --maxWorkers=4",
    "test:prod": "yarn build:tsc:test && jest --config jest.config.js --verbose=false --bail=true --maxWorkers=4",
    "version": "echo $npm_package_version"
  },
  "dependencies": {
    "@gms/common-graphql": "0.1.0",
    "@gms/common-util": "0.1.0",
    "@gms/ui-util": "0.1.0",
    "apollo-cache": "1.3.2",
    "apollo-cache-hermes": "0.8.10",
    "apollo-cache-inmemory": "1.6.3",
    "apollo-client": "2.6.4",
